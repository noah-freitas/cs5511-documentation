<!DOCTYPE HTML>
<html>
<head>
<meta charset="UTF-8">
<title>Courses</title>
<link href="bootstrap.css" rel="stylesheet" type="text/css">
<link href="courses.css" rel="stylesheet" type="text/css">
</head>

<body>
<div class="container">
<form class="form-horizontal" action="#" id="studentForm">
    <legend>Learn Web Design Online &#64; Santa Rosa Junior College</legend>
    <div class="control-group">
    <label class="control-label" for="inputName">Name</label>
    <div class="controls">
      <input type="text" id="inputName" placeholder="Student Name" required pattern="^(\w+)\s+(?:(\w+)\s+){0,1}(\w+){0,1}$">
    </div>

    <label class="control-label" for="inputEmail">Email</label>
    <div class="controls">
      <input type="email" id="inputEmail" placeholder="Email" required>
    </div>
  </div> 
  <div class="control-group">
    <div class="controls">
      <button type="submit" class="btn btn-primary" id="getStudent" form="studentForm">Get Started</button>
    </div>
  </div>
</form>
 <section id="schedule"><section id="courseList"></section><section id="sectionList"></section></section>
</div>
<script type="text/javascript" src="jquery.js"></script>
<script type="text/javascript" src="courses.js"></script>
</body>
</html>
